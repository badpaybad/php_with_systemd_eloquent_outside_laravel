{
    "require": {
        "php": "*.*",
        "goldspecdigital/laravel-eloquent-uuid": "^8.0",
        "illuminate/database": "*.*",
        "jenssegers/mongodb": "^3.8",
        "ramsey/uuid": "^4.1",
        "vlucas/phpdotenv": "^5.3"
    },
    "autoload": {
        "classmap": [
            "database/"
        ]         
        ,
        "psr-4": {
            "libs\\": "libs/",
            "app\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
